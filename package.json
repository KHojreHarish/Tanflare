{
  "name": "tanflare",
  "version": "1.0.0",
  "description": "Production-ready full-stack template with TanStack ecosystem, Cloudflare Workers, authentication, email, and enterprise-grade security",
  "keywords": [
    "tanstack",
    "cloudflare",
    "trpc",
    "react",
    "typescript",
    "template",
    "starter",
    "fullstack",
    "authentication",
    "email",
    "security",
    "csp",
    "rate-limiting"
  ],
  "author": "Your Name <your-email@example.com>",
  "license": "MIT",
  "homepage": "https://github.com/your-username/tanflare#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/tanflare.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/tanflare/issues"
  },
  "private": false,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "vite dev --host",
    "start": "wrangler dev",
    "build": "vite build",
    "check": "prettier --write . && eslint --fix",
    "deploy": "pnpm run build && wrangler deploy",
    "deploy:staging": "pnpm run build && wrangler deploy --env=staging",
    "deploy:prod": "pnpm run build && wrangler deploy --env=production",
    "cf:typegen": "wrangler types",
    "drizzle:gen": "drizzle-kit generate",
    "drizzle:studio": "pnpm drizzle-kit studio",
    "email:dev": "pnpm exec email dev --dir src/lib/email/templates --port 3001"
  },
